{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "env": {}
    },
    "linkedin_mcp_fps": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://linkedin-mcp-fps.onrender.com/sse"
      ]
    },
    "fps_cv_mcp": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "http://fps-cv.onrender.com/mcp"
      ]
    }
  }
}
